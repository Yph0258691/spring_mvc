<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <classPathEntry location="D:\project\java\respository\mysql\mysql-connector-java\8.0.19\mysql-connector-java-8.0.19.jar"/>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <commentGenerator>
            <property name="suppressDate" value="true" />
            <property name="suppressAllComments" value="true" />
        </commentGenerator>

        <!--serverTimezone=GMT 时区设置-->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/test?serverTimezone=GMT"
                        userId="root" password="root">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <!-- java 模型对象生成路径 -->
        <javaModelGenerator targetPackage="com.qs.acti.activity.model"
                            targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <!-- java mapper xml生成路径 -->
        <sqlMapGenerator targetPackage="game"
                         targetProject="src/main/resources/mybatis/mapper/">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <!--接口文件mapper java -->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.qs.acti.activity.mapper"
                             targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
            <!--property name="rootInterface" value="com.qs.common.base.basemapper.IBaseMapper"-/-->
        </javaClientGenerator>

        <!--数据库名-->
        <table schema="test" tableName="reward_raffle" domainObjectName="RewardRaffle" enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false">
        </table>
    </context>
</generatorConfiguration> 